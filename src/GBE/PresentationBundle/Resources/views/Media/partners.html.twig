    <!-- END leftbar -->

	<!-- BEGIN CONTENT -->
	<div class="col-md-8 page-content">  <!-- Must have a col-md-8 -->
		<!-- BEGIN CONTENT -->
		<div class="content">

			<div>
				<a href="{{ path('gbe_presentation_contact') }}" class="btn btn-perso float-right">
			    	{{ 'index.actions.becomePartner'|trans }}
